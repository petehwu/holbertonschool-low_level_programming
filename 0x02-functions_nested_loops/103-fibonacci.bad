#include <stdio.h>

/**
 * main - print first 50 fibonacci numbers
 * Return: always 0
 */

int main(void)
{
	long int y;
	long int z;
	long int tot;
	long int x;

	y = 1;
	z = 2;
	x = 0;
	while (z < 4000000)
	{
		if (z % 2 == 0)
			x += z;
		tot = y + z;
		y = z;
		z = tot;
		printf("%ld, ", y);
		printf("%ld, ", z);
	}
	printf("%ld\n", z);
	return (0);
}
